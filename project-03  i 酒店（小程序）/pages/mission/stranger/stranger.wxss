/* pages/mission/stranger/stranger.wxss */ 
page{
  padding: 10rpx 0 60rpx 0 ;
}
/* 上方 */
.pos_view{
  width: 100%;
  position: fixed;
  top: 0;
  left:0;
  z-index: 9;
}
.header{
  background-color: #ffffff;
  border-top: 1rpx solid #eeeeee;
  border-bottom: 1rpx solid #eeeeee;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
  position: relative;
}
/* 表格展示数据 */
.section{
  overflow: hidden;
  margin: 20rpx auto;
  box-shadow:0 2rpx 6rpx 0rpx rgba(204,204,204,1);
  border-radius:10rpx;
}
.section ul{
  width: 100%;
  font-size: 24rpx;
  background-color: #FFFFFF;
  border-bottom: 1rpx solid #eeeeee;
}
.section ul li{
  flex: 1;
  line-height: 60rpx;
  text-align: center;
} 
.section .list_style{
  color: #777777;
}
.section .date_color{
  color: #2080DE;
}
